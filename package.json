{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop --open",
    "start": "npm run develop",
    "build": "gatsby build",
    "deploy": "npm-run-all build cname surge",
    "surge": "surge public/",
    "cname": "ncp static/CNAME public/CNAME",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.25",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-remark-bracketed-spans": "^2.0.3",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.19",
    "gatsby-transformer-sharp": "^2.1.10",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "surge": "^0.20.1"
  }
}
